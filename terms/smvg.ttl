@prefix rdf: <https://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <https://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

smvg:categories
    a rdf:Property ;
        rdfs:label "categories"@en ;
        rdfs:comment "List of categories that the game belongs to."@en ;
        rdfs:range xsd:string ;

smvg:achievements
    a rdf:Property ;
        rdfs:label "achievements"@en ;
        rdfs:comment "Number of achievements available to win in the game."@en ;
        rdfs:range xsd:integer ;

smvg:avg_playtime
    a rdf:Property ;
        rdfs:label "average playtime"@en ;
        rdfs:comment "Average playtime in hours."@en ;
        rdfs:range xsd:float ;

smvg:median_playtime
    a rdf:Property ;
        rdfs:label "median playtime"@en ;
        rdfs:comment "Median playtime in hours."@en ;
        rdfs:range xsd:float ;

smvg:owners
    a rdf:Property ;
        rdfs:label "owners"@en ;
        rdfs:comment "Number of owners of the game."@en ;
        rdfs:range xsd:integer ;

